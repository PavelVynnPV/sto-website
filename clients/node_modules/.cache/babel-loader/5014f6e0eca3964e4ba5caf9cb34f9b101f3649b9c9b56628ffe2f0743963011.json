\",{}),\" \",/*#__PURE__*/_jsx(\"strong\",{children:\"\\u0417 \\u043F\\u043E\\u0432\\u0430\\u0433\\u043E\\u044E, \\u043A\\u043E\\u043B\\u0435\\u043A\\u0442\\u0438\\u0432 \\u0421\\u0422\\u041E \\xABANDRCARS\\xBB\"})]})]})}),/*#__PURE__*/_jsx(\"section\",{className:styles.bottom_slider,children:/*#__PURE__*/_jsx(SwiperBottomSlider,{})}),/*#__PURE__*/_jsx(\"iframe\",{src:\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2542.1616811428316!2d30.425691999999998!3d50.4194592!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40d4c955dc751c8f%3A0x6215c067abe7eebc!2z0J7RgtGA0LDQtNC90YvQuSDQv9GALdGCLiwgOTUsINCa0LjQtdCyLCAwMjAwMA!5e0!3m2!1sru!2sua!4v1686730729672!5m2!1sru!2sua\",width:\"100%\",height:\"500\",style:{border:\"0\"},allowfullscreen:\"\",loading:\"lazy\",referrerpolicy:\"no-referrer-when-downgrade\",title:\"place\"})]});};export default Main;","map":{"version":3,"names":["React","styles","Link","SwiperBottomSlider","SwiperSlider","jsx","_jsx","jsxs","_jsxs","Fragment","_Fragment","Main","children","className","advances","advances__container","advances__title","advances__blocks_container","advances__block","advances__man_icon","advances__text_container","advances_block__title","advances_block__text","advances__time_icon","advances__secure_icon","advances__repair_icon","fix_kinds","fix__blocks_container","to","fix__block","fix__block_text","diagnostic","diagnostic__container","diagnostic__text","bottom_slider","src","width","height","style","border","allowfullscreen","loading","referrerpolicy","title"],"sources":["C:/Users/Pavel/Desktop/portfolio/sto-webs/clients/src/components/Main/Main.jsx"],"sourcesContent":["import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { SwiperBottomSlider, SwiperSlider } from \"../SwiperSlider\";\r\n\r\nconst Main = () => {\r\n  return (\r\n    <>\r\n      <SwiperSlider />\r\n      <section className={styles.advances}>\r\n        <div className={styles.advances__container}>\r\n          <h1 className={styles.advances__title}>\r\n            Чому варте співпрацювати з СТО «ANDRCARS»\r\n          </h1>\r\n          <div className={styles.advances__blocks_container}>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__man_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Професійна консультація\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Вас обслуговують тілько професійні майстри, які доступно и\r\n                  повністю пояснять вашу проблему\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__time_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Ремонт точно в строк\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Ваш автомобіль буде готовий, в обговорений з майстром час\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__secure_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Гарантія на послуги\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Ми надаєм повну гарантію на послуги, які були надані нашим СТО\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__repair_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Оригінальні автозапчастини\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Ми володіємо всіма сертифікатами від найбільших постачальників\r\n                  автозапчастин в Україні\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n      <section className={styles.fix_kinds}>\r\n        <h1 className={styles.advances__title}>\r\n          СТО «ANDRCARS» всі види ремонту та технічного обслуговування\r\n          <br /> автомобілів\r\n        </h1>\r\n        <p>\r\n          Мы предоставляем качественные услуги по диагностике и техническому\r\n          обслуживанию авто в Киеве. Специалисты с многолетним опытом выполняют\r\n          самые сложные задачи.\r\n        </p>\r\n        <div className={styles.fix__blocks_container}>\r\n          <div>\r\n          <Link to=\"/autodiagnostic\" className={styles.fix__block}>\r\n            <p className={styles.fix__block_text}>Автодіагностика</p>\r\n          </Link>\r\n          <Link to=\"/tehservice\" className={styles.fix__block}>\r\n            <p className={styles.fix__block_text}>Технічне обслуговування</p>\r\n          </Link>\r\n          <Link to=\"/electric\" className={styles.fix__block}>\r\n            <p className={styles.fix__block_text}>Автоелектрика</p>\r\n          </Link>\r\n          </div>\r\n          <div>\r\n          <Link to=\"/hodovaya\" className={styles.fix__block}>\r\n            <p className={styles.fix__block_text}>Ремонт ходової</p>\r\n          </Link>\r\n          <Link to=\"/otherservices\" className={styles.fix__block}>\r\n            <p className={styles.fix__block_text}>Інші послуги автосервісу</p>\r\n          </Link>\r\n          </div>\r\n        </div>\r\n      </section>\r\n      <section className={styles.diagnostic}>\r\n        <div className={styles.diagnostic__container}>\r\n          <h1 className={styles.advances__title}>\r\n            Діагностика і ремонт автомобіля в Києві\r\n          </h1>\r\n          <p className={styles.diagnostic__text}>\r\n            Для більшості людей автомобіль стає невід'ємною частиною і в якомусь\r\n            сенсі найкращим другом. Який мовчазно допомагає у найважчих життєвих\r\n            обставинах. Тому логічно, коли і ви допомагаєте своєму другові\r\n            завжди залишатися бадьорим і здоровим, то і він у свою чергу вам за\r\n            це безмежно віддячить.\r\n            <br />\r\n            <br /> Колектив СТО «ANDRCARS» із задоволенням та професіоналізмом\r\n            допоможе вам тримати вашого «залізного коня» у чудовій формі. А вам\r\n            залишається лише не забувати періодично заїжджати до нас.\r\n            <br />\r\n            <br /> СТО \"ANDRCARS\" займається ремонтом автомобілів впродовж\r\n            тривалого часу. Ми нагромадили величезний досвід у роботі та навчили\r\n            багато висококласних фахівців для свого автосервісу.\r\n            <br />\r\n            <br /> Наше СТО є мультибрендовим. Ми професійно робимо ремонт\r\n            комерційних та легкових автомобілів різних виробників. Спектр\r\n            послуг, які ми надаємо та кількість спеціального обладнання –\r\n            величезний. Тому, звертаючись до нас, вам не доведеться бігати по\r\n            всьому місту для вирішення вашої проблеми.\r\n            <br />\r\n            <br />\r\n            Також на нашому СТО вам завжди допоможуть з підбором та доставкою\r\n            автозапчастин на ваш автомобіль. Хочу відмітити за гарною ціною та\r\n            високою якістю. У нас завжди велика наявність запчастин на складі, а\r\n            також швидка доставка зі складів наших партнерів.\r\n            <br />\r\n            <br /> <strong>З повагою, колектив СТО «ANDRCARS»</strong>\r\n          </p>\r\n        </div>\r\n      </section>\r\n      <section className={styles.bottom_slider}>\r\n        <SwiperBottomSlider />\r\n      </section>\r\n      <iframe\r\n        src=\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2542.1616811428316!2d30.425691999999998!3d50.4194592!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40d4c955dc751c8f%3A0x6215c067abe7eebc!2z0J7RgtGA0LDQtNC90YvQuSDQv9GALdGCLiwgOTUsINCa0LjQtdCyLCAwMjAwMA!5e0!3m2!1sru!2sua!4v1686730729672!5m2!1sru!2sua\"\r\n        width=\"100%\"\r\n        height=\"500\"\r\n        style={{ border: \"0\" }}\r\n        allowfullscreen=\"\"\r\n        loading=\"lazy\"\r\n        referrerpolicy=\"no-referrer-when-downgrade\"\r\n        title=\"place\"\r\n      ></iframe>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Main;\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,CAAAC,MAAM,KAAM,2BAA2B,CAC9C,OAASC,IAAI,KAAQ,kBAAkB,CACvC,OAASC,kBAAkB,CAAEC,YAAY,KAAQ,iBAAiB,CAAC,OAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,gCAAAC,QAAA,IAAAC,SAAA,yBAEnE,GAAM,CAAAC,IAAI,CAAG,QAAP,CAAAA,IAAIA,CAAA,CAAS,CACjB,mBACEH,KAAA,CAAAE,SAAA,EAAAE,QAAA,eACEN,IAAA,CAACF,YAAY,GAAE,CAAC,cAChBE,IAAA,YAASO,SAAS,CAAEZ,MAAM,CAACa,QAAS,CAAAF,QAAA,cAClCJ,KAAA,QAAKK,SAAS,CAAEZ,MAAM,CAACc,mBAAoB,CAAAH,QAAA,eACzCN,IAAA,OAAIO,SAAS,CAAEZ,MAAM,CAACe,eAAgB,CAAAJ,QAAA,CAAC,mLAEvC,CAAI,CAAC,cACLJ,KAAA,QAAKK,SAAS,CAAEZ,MAAM,CAACgB,0BAA2B,CAAAL,QAAA,eAChDJ,KAAA,QAAKK,SAAS,CAAEZ,MAAM,CAACiB,eAAgB,CAAAN,QAAA,eACrCN,IAAA,SAAMO,SAAS,CAAEZ,MAAM,CAACkB,kBAAmB,CAAO,CAAC,cACnDX,KAAA,QAAKK,SAAS,CAAEZ,MAAM,CAACmB,wBAAyB,CAAAR,QAAA,eAC9CN,IAAA,OAAIO,SAAS,CAAEZ,MAAM,CAACoB,qBAAsB,CAAAT,QAAA,CAAC,uIAE7C,CAAI,CAAC,cACLN,IAAA,MAAGO,SAAS,CAAEZ,MAAM,CAACqB,oBAAqB,CAAAV,QAAA,CAAC,keAG3C,CAAG,CAAC,EACD,CAAC,EACH,CAAC,cACNJ,KAAA,QAAKK,SAAS,CAAEZ,MAAM,CAACiB,eAAgB,CAAAN,QAAA,eACrCN,IAAA,SAAMO,SAAS,CAAEZ,MAAM,CAACsB,mBAAoB,CAAO,CAAC,cACpDf,KAAA,QAAKK,SAAS,CAAEZ,MAAM,CAACmB,wBAAyB,CAAAR,QAAA,eAC9CN,IAAA,OAAIO,SAAS,CAAEZ,MAA