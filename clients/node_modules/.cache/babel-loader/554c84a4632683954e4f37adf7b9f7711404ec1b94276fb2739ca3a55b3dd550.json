ce__content","steering__banner","service__banner_title","service__info","service__title","service__text","service"],"sources":["C:/Users/user/Desktop/portfolio/sto-webs/clients/src/components/SimpleLinks/Steering.jsx"],"sourcesContent":["import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport ServiceComponent from \"./ServiceComponent\";\r\nimport { steering } from \"./helper\";\r\n\r\nconst Steering = () => {\r\n  return (\r\n    <>\r\n      <section className={styles.service__block}>\r\n        <div className={styles.service__content}>\r\n          <div className={styles.steering__banner}>\r\n            <h1 className={styles.service__banner_title}>Рульове управління</h1>\r\n          </div>\r\n          <div className={styles.service__info}>\r\n            <h2 className={styles.service__title}>Рульове управління</h2>\r\n\r\n            <p className={styles.service__text}>\r\n              Наша компанія займається ремонтом рульового управління, що є\r\n              важливою складовою частиною системи керування автомобілем. Рульове\r\n              управління включає в себе рульову колонку, рейку керма, насос\r\n              гідропідсилювача (якщо присутній), рульові тяги та інші\r\n              компоненти.\r\n              <br />\r\n              <br />\r\n              Наші кваліфіковані механіки проводять діагностику рульового\r\n              управління для виявлення можливих проблем, таких як знос,\r\n              пошкодження або несправність компонентів. Ми здійснюємо ремонт\r\n              рульової колонки, заміну рейки керма, налагодження і ремонт\r\n              гідропідсилювача (якщо присутній), заміну рульових тяг та інших\r\n              необхідних деталей.\r\n              <br />\r\n              <br />\r\n              Ми використовуємо лише якісні запасні частини та сучасне\r\n              обладнання для проведення ремонту рульового управління. Наші\r\n              експерти забезпечать правильне налаштування та функціонування\r\n              рульової системи, що дозволить вам керувати автомобілем з\r\n              комфортом і безпекою.\r\n              <br />\r\n              <br />\r\n              Звертайтеся до нас для професійного ремонту рульового управління\r\n              вашого автомобіля. Ми зробимо все можливе, щоб відновити його\r\n              оптимальну роботу та забезпечити вам безпечну керованість\r\n              автомобілем.\r\n            </p>\r\n            <ServiceComponent service={steering} />\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Steering;\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,CAAAC,MAAM,KAAM,2BAA2B,CAC9C,MAAO,CAAAC,gBAAgB,KAAM,oBAAoB,CACjD,OAASC,QAAQ,KAAQ,UAAU,CAAC,OAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,gCAAAC,QAAA,IAAAC,SAAA,yBAEpC,GAAM,CAAAC,QAAQ,CAAG,QAAX,CAAAA,QAAQA,CAAA,CAAS,CACrB,mBACEL,IAAA,CAAAI,SAAA,EAAAE,QAAA,cACEN,IAAA,YAASO,SAAS,CAAEX,MAAM,CAACY,cAAe,CAAAF,QAAA,cACxCJ,KAAA,QAAKK,SAAS,CAAEX,MAAM,CAACa,gBAAiB,CAAAH,QAAA,eACtCN,IAAA,QAAKO,SAAS,CAAEX,MAAM,CAACc,gBAAiB,CAAAJ,QAAA,cACtCN,IAAA,OAAIO,SAAS,CAAEX,MAAM,CAACe,qBAAsB,CAAAL,QAAA,CAAC,yGAAkB,CAAI,CAAC,CACjE,CAAC,cACNJ,KAAA,QAAKK,SAAS,CAAEX,MAAM,CAACgB,aAAc,CAAAN,QAAA,eACnCN,IAAA,OAAIO,SAAS,CAAEX,MAAM,CAACiB,cAAe,CAAAP,QAAA,CAAC,yGAAkB,CAAI,CAAC,cAE7DJ,KAAA,MAAGK,SAAS,CAAEX,MAAM,CAACkB,aAAc,CAAAR,QAAA,EAAC,q0CAMlC,cAAAN,IAAA,QAAK,CAAC,cACNA,IAAA,QAAK,CAAC,grDAON,cAAAA,IAAA,QAAK,CAAC,cACNA,IAAA,QAAK,CAAC,22CAMN,cAAAA,IAAA,QAAK,CAAC,cACNA,IAAA,QAAK,CAAC,0hCAKR,EAAG,CAAC,cACJA,IAAA,CAACH,gBAAgB,EAACkB,OAAO,CAAEjB,QAAS,CAAE,CAAC,EACpC,CAAC,EACH,CAAC,CACC,CAAC,CACV,CAAC,CAEP,CAAC,CAED,cAAe,CAAAO,QAAQ"},"metadata":{},"sourceType":"module","externalDependencies":[]}