aking_system","jsx","_jsx","jsxs","_jsxs","Fragment","_Fragment","BrakSystem","children","className","service__block","service__content","break__banner","service__banner_title","service__info","service__title","service__text","service"],"sources":["C:/Users/user/Desktop/portfolio/sto-webs/clients/src/components/SimpleLinks/BrakSystem.jsx"],"sourcesContent":["import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport ServiceComponent from \"./ServiceComponent\";\r\nimport { braking_system } from \"./helper\";\r\n\r\nconst BrakSystem = () => {\r\n  return (\r\n    <>\r\n      <section className={styles.service__block}>\r\n        <div className={styles.service__content}>\r\n          <div className={styles.break__banner}>\r\n            <h1 className={styles.service__banner_title}>Гальмівна система</h1>\r\n          </div>\r\n          <div className={styles.service__info}>\r\n            <h2 className={styles.service__title}>Гальмівна система</h2>\r\n            <p className={styles.service__text}>\r\n              Наша компанія пропонує послугу ремонту гальмівної системи, яка\r\n              відіграє найважливішу роль у забезпеченні безпеки на дорозі. Наші\r\n              кваліфіковані механіки проводять комплексну перевірку гальмівної\r\n              системи для виявлення потенційних проблем, таких як знос\r\n              гальмівних колодок, пошкодження гальмівних дисків, проблеми з\r\n              гальмівними шлангами або сполуками.\r\n              <br />\r\n              <br />\r\n              Ми використовуємо високоякісні запасні частини та сучасне\r\n              обладнання для здійснення ремонту гальмівної системи. Наші\r\n              експерти усувають проблеми, проводять заміну гальмівних колодок,\r\n              дисків, тормозних циліндрів, регулюють гальмівний баланс та\r\n              здійснюють всі необхідні налаштування.\r\n              <br />\r\n              <br />\r\n              Ми рекомендуємо регулярну перевірку та обслуговування гальмівної\r\n              системи для забезпечення її оптимальної роботи і надійності.\r\n              Важливо вчасно виявляти і виправляти будь-які проблеми, що можуть\r\n              впливати на ефективність гальмування та безпеку під час руху.\r\n              <br />\r\n              <br />\r\n              Звертайтеся до нас для професійного ремонту гальмівної системи\r\n              вашого автомобіля. Наші експерти забезпечать надійну роботу\r\n              гальмів та забезпечать вашу безпеку на дорозі.\r\n            </p>\r\n\r\n            <ServiceComponent service={braking_system} />\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default BrakSystem;\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,CAAAC,MAAM,KAAM,2BAA2B,CAC9C,MAAO,CAAAC,gBAAgB,KAAM,oBAAoB,CACjD,OAASC,cAAc,KAAQ,UAAU,CAAC,OAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,gCAAAC,QAAA,IAAAC,SAAA,yBAE1C,GAAM,CAAAC,UAAU,CAAG,QAAb,CAAAA,UAAUA,CAAA,CAAS,CACvB,mBACEL,IAAA,CAAAI,SAAA,EAAAE,QAAA,cACEN,IAAA,YAASO,SAAS,CAAEX,MAAM,CAACY,cAAe,CAAAF,QAAA,cACxCJ,KAAA,QAAKK,SAAS,CAAEX,MAAM,CAACa,gBAAiB,CAAAH,QAAA,eACtCN,IAAA,QAAKO,SAAS,CAAEX,MAAM,CAACc,aAAc,CAAAJ,QAAA,cACnCN,IAAA,OAAIO,SAAS,CAAEX,MAAM,CAACe,qBAAsB,CAAAL,QAAA,CAAC,mGAAiB,CAAI,CAAC,CAChE,CAAC,cACNJ,KAAA,QAAKK,SAAS,CAAEX,MAAM,CAACgB,aAAc,CAAAN,QAAA,eACnCN,IAAA,OAAIO,SAAS,CAAEX,MAAM,CAACiB,cAAe,CAAAP,QAAA,CAAC,mGAAiB,CAAI,CAAC,cAC5DJ,KAAA,MAAGK,SAAS,CAAEX,MAAM,CAACkB,aAAc,CAAAR,QAAA,EAAC,+zDAOlC,cAAAN,IAAA,QAAK,CAAC,cACNA,IAAA,QAAK,CAAC,09CAMN,cAAAA,IAAA,QAAK,CAAC,cACNA,IAAA,QAAK,CAAC,6zCAKN,cAAAA,IAAA,QAAK,CAAC,cACNA,IAAA,QAAK,CAAC,s4BAIR,EAAG,CAAC,cAEJA,IAAA,CAACH,gBAAgB,EAACkB,OAAO,CAAEjB,cAAe,CAAE,CAAC,EAC1C,CAAC,EACH,CAAC,CACC,CAAC,CACV,CAAC,CAEP,CAAC,CAED,cAAe,CAAAO,UAAU"},"metadata":{},"sourceType":"module","externalDependencies":[]}