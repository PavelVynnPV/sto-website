/components/SimpleLinks/Services\";\r\n\r\nconst AppRoutes = ({ href }) => {\r\n  const [windowLocHref, setWindowLocHref] = useState(\"\");\r\n  const [loc, setLoc] = useState(\"http://localhost:3000/\");\r\n  let paths = loc.pathname?.split(\"/\").filter((path) => path !== \"\");\r\n\r\n  return (\r\n    <>\r\n      <BrowserRouter>\r\n        <ScrollToTop />\r\n        <Navbar />\r\n        <LinksBar\r\n          href={href}\r\n          setWindowLocHref={setWindowLocHref}\r\n          windowLocHref={windowLocHref}\r\n          setLoc={setLoc}\r\n        />\r\n        <Routes>\r\n          <Route exact path=\"/sto-website\" element={<Main />} />\r\n          <Route exact path=\"/contacts\" element={<Contacts />} />\r\n          {/* <Route exact path=\"/hodovaya\" element={<HodovayaFix />} /> */}\r\n          {/* <Route exact path=\"/autodiagnostic\" element={<AutoDiagnostic />} /> */}\r\n          <Route exact path=\"/tehservice\" element={<TehService />} />\r\n          <Route exact path=\"/otherservices\" element={<OtherServices />} />\r\n          <Route exact path=\"/comments\" element={<CommentForm />} />\r\n          <Route exact path=\"/autoconditioner\" element={<AutoConditioner />} />\r\n          <Route exact path=\"/tirefitting\" element={<TireFitting />} />\r\n          <Route exact path=\"/rozval\" element={<Rozval />} />\r\n          {/* <Route exact path=\"/engine\" element={<Engine />} /> */}\r\n          {/* <Route exact path=\"/tehfluids\" element={<TehFluids />} /> */}\r\n          {/* <Route exact path=\"/braksystem\" element={<BrakSystem />} /> */}\r\n          {/* <Route exact path=\"/steering\" element={<Steering />} /> */}\r\n          <Route\r\n            exact\r\n            path={`/${paths}`}\r\n            element={\r\n              paths !== \"/allservices\" ? (\r\n                <Services />\r\n              ) : paths !== \"/allservices\" ? (\r\n                <Services />\r\n              ) : null\r\n            }\r\n          />\r\n        </Routes>\r\n        <Footer />\r\n      </BrowserRouter>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default AppRoutes;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Navbar\\index.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Main\\index.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Comments\\index.js",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\index.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Footer\\index.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Navbar\\Navbar.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Main\\Main.jsx",["206"],[],"import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { SwiperBottomSlider, SwiperSlider } from \"../SwiperSlider\";\r\nimport AllServices from \"./AllServices\";\r\n\r\nconst Main = () => {\r\n  return (\r\n    <>\r\n      <SwiperSlider />\r\n      <section className={styles.advances}>\r\n        <div className={styles.advances__container}>\r\n          <h1 className={styles.advances__title}>\r\n            Чому варто співпрацювати з СТО «ANDRCARS»\r\n          </h1>\r\n          <div className={styles.advances__blocks_container}>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__man_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Професійна консультація\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Вас обслуговують тільки професійні майстри, які доступно і\r\n                  повністю пояснять вашу проблему\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__time_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Ремонт точно в строк\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Ваш автомобіль буде готовий, в обговорений з майстром час\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__secure_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Гарантія на послуги\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Ми надаєм повну гарантію на послуги, які були надані нашим СТО\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className={styles.advances__block}>\r\n              <span className={styles.advances__repair_icon}></span>\r\n              <div className={styles.advances__text_container}>\r\n                <h1 className={styles.advances_block__title}>\r\n                  Оригінальні автозапчастини\r\n                </h1>\r\n                <p className={styles.advances_block__text}>\r\n                  Ми володіємо всіма сертифікатами від найбільших постачальників\r\n                  автозапчастин в Україні\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n      <section className={styles.fix_kinds}>\r\n        <h1 className={styles.advances__title}>\r\n          СТО «ANDRCARS» всі види ремонту та технічного обслуговування\r\n          <br /> автомобілів\r\n        </h1>\r\n        <p>\r\n          Ми надаємо якісні послуги з діагностики та технічного обслуговування\r\n          авто в Києві. Фахівці з багаторічним досвідом виконують найскладніші\r\n          завдання.\r\n        </p>\r\n        <AllServices />\r\n      </section>\r\n      <section className={styles.diagnostic}>\r\n        <div className={styles.diagnostic__container}>\r\n          <h1 className={styles.advances__title}>\r\n            Діагностика і ремонт автомобіля в Києві\r\n          </h1>\r\n          <p className={styles.diagnostic__text}>\r\n            Для більшості людей автомобіль стає невід'ємною частиною і в якомусь\r\n            сенсі найкращим другом. Який мовчазно допомагає у найважчих життєвих\r\n            обставинах. Тому логічно, коли і ви допомагаєте своєму другові\r\n            завжди залишатися бадьорим і здоровим, то і він у свою чергу вам за\r\n            це безмежно віддячить.\r\n            <br />\r\n            <br /> Колектив СТО «ANDRCARS» із задоволенням та професіоналізмом\r\n            допоможе вам тримати вашого «залізного коня» у чудовій формі. А вам\r\n            залишається лише не забувати періодично заїжджати до нас.\r\n            <br />\r\n            <br /> СТО \"ANDRCARS\" займається ремонтом автомобілів впродовж\r\n            тривалого часу. Ми нагромадили величезний досвід у роботі та навчили\r\n            багато висококласних фахівців для свого автосервісу.\r\n            <br />\r\n            <br /> Наше СТО є мультибрендовим. Ми професійно робимо ремонт\r\n            комерційних та легкових автомобілів різних виробників. Спектр\r\n            послуг, які ми надаємо та кількість спеціального обладнання –\r\n            величезний. Тому, звертаючись до нас, вам не доведеться бігати по\r\n            всьому місту для вирішення вашої проблеми.\r\n            <br />\r\n            <br />\r\n            Також на нашому СТО вам завжди допоможуть з підбором та доставкою\r\n            автозапчастин на ваш автомобіль. Хочу відмітити за гарною ціною та\r\n            високою якістю. У нас завжди велика наявність запчастин на складі, а\r\n            також швидка доставка зі складів наших партнерів.\r\n            <br />\r\n            <br /> <strong>З повагою, колектив СТО «ANDRCARS»</strong>\r\n          </p>\r\n        </div>\r\n      </section>\r\n      <section className={styles.bottom_slider}>\r\n        <SwiperBottomSlider />\r\n      </section>\r\n      <iframe\r\n        src=\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2542.1616811428316!2d30.425691999999998!3d50.4194592!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40d4c955dc751c8f%3A0x6215c067abe7eebc!2z0J7RgtGA0LDQtNC90YvQuSDQv9GALdGCLiwgOTUsINCa0LjQtdCyLCAwMjAwMA!5e0!3m2!1sru!2sua!4v1688729927932!5m2!1sru!2sua\"\r\n        width=\"100%\"\r\n        height=\"500\"\r\n        style={{ border: \"0\" }}\r\n        allowfullscreen=\"\"\r\n        loading=\"lazy\"\r\n        title=\"place\"\r\n        referrerpolicy=\"no-referrer-when-downgrade\"\r\n      ></iframe>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Main;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Navbar\\LinksBar.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Comments\\CommentForm.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Comments\\CommentsList.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\HodovayaFix.jsx",["207"],[],"import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport { hodova, braking_system } from \"./helper\";\r\nimport ServiceComponent from \"./ServiceComponent\";\r\n\r\nconst Hodovaya = () => {\r\n  return (\r\n    <section className={styles.service__block}>\r\n      <div className={styles.service__content}>\r\n        <div className={styles.hodovaya__banner}>\r\n          <h1 className={styles.service__banner_title}>\r\n            Послуги з ремонту ходової\r\n          </h1>\r\n        </div>\r\n        <div className={styles.service__info}>\r\n          <h2 className={styles.service__title}>Ходова частина</h2>\r\n          <p className={styles.service__text}></p>\r\n          Наша компанія спеціалізується на ремонті ходової частини автомобіля,\r\n          що включає в себе компоненти, які відповідають за стійкість,\r\n          керованість і комфорт під час руху. Наші кваліфіковані механіки\r\n          здійснюють комплексну перевірку та діагностику ходової частини для\r\n          виявлення можливих проблем, таких як знос, пошкодження або\r\n          несправність компонентів.\r\n          <br />\r\n          <br />\r\n          Ми займаємося ремонтом рульової системи, підвіски, амортизаторів,\r\n          пружин, стійок, тормозної системи та інших складових ходової частини.\r\n          Ми використовуємо високоякісні запасні частини та сучасне обладнання,\r\n          щоб забезпечити надійність та оптимальну роботу ходової частини.\r\n          <br />\r\n          <br />\r\n          Після діагностики ми надаємо вам докладний звіт про стан ходової\r\n          частини, рекомендації щодо необхідних ремонтних робіт та запасних\r\n          частин. Наші експерти виконають ремонт з високою професіоналізмом та\r\n          уважністю до деталей, забезпечуючи безпеку та комфорт під час\r\n          керування автомобілем.\r\n          <br />\r\n          <br />\r\n          Звертайтеся до нас для професійного ремонту ходової частини вашого\r\n          автомобіля. Ми зробимо все необхідне, щоб відновити оптимальну роботу\r\n          ходової частини і забезпечити безпеку та комфорт під час їзди.\r\n          <ServiceComponent service={hodova} />\r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default Hodovaya;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\AutoDiagnostic.jsx",["208"],[],"import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport {\r\n  car_diagnostics1,\r\n  car_diagnostics2,\r\n  car_diagnostics3,\r\n  diagnostics_injection,\r\n  diagnostics_turbocharging,\r\n} from \"./helper\";\r\nimport ServiceComponent from \"./ServiceComponent\";\r\n\r\nconst AutoDiagnostic = () => {\r\n  return (\r\n    <section className={styles.service__block} id={styles.autoDiagnostic_block}>\r\n      <div className={styles.service__content}>\r\n        <div className={styles.autodiagnostic__banner}>\r\n          <h1 className={styles.service__banner_title}>\r\n            Послуги з автодіагностики\r\n          </h1>\r\n        </div>\r\n        <div className={styles.service__info}>\r\n          <h2 className={styles.service__title}>Діагностика автомобіля</h2>\r\n\r\n          <p className={styles.service__text}>\r\n            Наша компанія надає послугу діагностики автомобіля, щоб забезпечити\r\n            надійність та оптимальну роботу вашого транспортного засобу. Наші\r\n            кваліфіковані механіки та високотехнологічне обладнання допоможуть\r\n            виявити потенційні проблеми з системами автомобіля, такими як\r\n            двигун, електрична система, підвіска, гальма та інші. Після\r\n            проведення діагностики, ми зробимо повний звіт про стан вашого\r\n            автомобіля і надамо рекомендації щодо подальшого обслуговування або\r\n            ремонту. <br />\r\n            <br />\r\n            Зверніться до нас для професійної діагностики, щоб забезпечити\r\n            безпеку та ефективність вашого автомобіля.\r\n          </p>\r\n          <ServiceComponent service={car_diagnostics1} />\r\n\r\n          <ServiceComponent service={car_diagnostics2} />\r\n\r\n          <ServiceComponent service={car_diagnostics3} />\r\n\r\n          <h2 className={styles.service__title}>Діагностика турбонаддуву</h2>\r\n          <p className={styles.service__text}>\r\n            Наша компанія надає послугу діагностики турбонаддуву, щоб\r\n            забезпечити правильну роботу та оптимальну продуктивність турбінного\r\n            системи вашого автомобіля. Наші кваліфіковані спеціалісти здійснюють\r\n            комплексну перевірку всіх компонентів турбонаддуву, включаючи\r\n            турбокомпресор, втулку, паливний насос, датчики тиску та контролери.\r\n            Ми використовуємо спеціальні діагностичні пристрої для виявлення\r\n            можливих проблем, таких як витік або пошкодження, зменшення тиску,\r\n            недостатній надлишок або нерівномірна робота турбонаддуву. Після\r\n            діагностики ми надаємо повний звіт про стан системи турбонаддуву та\r\n            рекомендації щодо необхідного обслуговування або ремонту.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійної діагностики турбонаддуву і\r\n            забезпечуйте надійну та ефективну роботу вашого автомобіля.\r\n          </p>\r\n          <ServiceComponent service={diagnostics_turbocharging} />\r\n\r\n          <p className={styles.service__text}></p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default AutoDiagnostic;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\OtherServices.jsx",["209","210"],[],"import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport {\r\n  steering,\r\n  transmisia,\r\n  clutch,\r\n  exhaust_system,\r\n  cooling_system,\r\n  engine_repair,\r\n} from \"./helper\";\r\nimport ServiceComponent from \"./ServiceComponent\";\r\n\r\nconst OtherServices = () => {\r\n  return (\r\n    <section className={styles.service__block}>\r\n      <div className={styles.service__content}>\r\n        <div className={styles.other_services__banner}>\r\n          <h1 className={styles.service__banner_title}>\r\n            Інші послуги автосервісу\r\n          </h1>\r\n        </div>\r\n        <div className={styles.service__info}>\r\n        \r\n\r\n          <h2 className={styles.service__title}>Трансмісія</h2>\r\n\r\n          <p className={styles.service__text}>\r\n            Наша компанія спеціалізується на ремонті трансмісії автомобіля, що\r\n            відповідає за передачу потужності з двигуна на колеса. Трансмісія\r\n            складається з різних компонентів, таких як механічна коробка\r\n            передач, автоматична трансмісія, муфти, шестерні, ланцюги,\r\n            підшипники та інші деталі.\r\n            <br />\r\n            <br />\r\n            Наші висококваліфіковані техніки здійснюють діагностику трансмісії\r\n            для виявлення проблем, таких як знос деталей, пошкодження шестерень,\r\n            неправильна робота муфт або ланцюгів, витік оливи та інші\r\n            несправності. Після діагностики, ми пропонуємо докладний звіт про\r\n            стан трансмісії та рекомендації щодо ремонтних робіт.\r\n            <br />\r\n            <br />\r\n            Ми використовуємо якісні запасні частини, які відповідають\r\n            стандартам виробників, та сучасне обладнання для виконання ремонту\r\n            трансмісії. Наші експерти здійснюють заміну пошкоджених деталей,\r\n            ремонт механізмів та налагодження трансмісії для відновлення її\r\n            оптимальної роботи.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійного ремонту трансмісії вашого\r\n            автомобіля. Ми забезпечимо надійну та ефективну роботу трансмісії,\r\n            що дозволить вам насолоджуватися комфортною та безпечною їздою.\r\n          </p>\r\n          <ServiceComponent service={transmisia} />\r\n\r\n          <h2 className={styles.service__title}>Зчеплення</h2>\r\n\r\n          <p className={styles.service__text}>\r\n            Наша компанія надає послуги з ремонту зчеплення, яке відіграє\r\n            важливу роль у передачі потужності з двигуна на трансмісію\r\n            автомобіля. Зчеплення складається з диска зчеплення, пресуючого\r\n            механізму, вижимного підшипника та інших компонентів.\r\n            <br />\r\n            <br />\r\n            Наші висококваліфіковані техніки проводять діагностику зчеплення для\r\n            виявлення можливих проблем, таких як знос диска зчеплення,\r\n            несправність пресуючого механізму або вижимного підшипника,\r\n            неправильна робота зчеплення та інші несправності. Після\r\n            діагностики, ми рекомендуємо необхідний обсяг ремонтних робіт та\r\n            заміну деталей.\r\n            <br />\r\n            <br />\r\n            Ми використовуємо високоякісні запасні частини, що відповідають\r\n            стандартам виробників, та сучасне обладнання для виконання ремонту\r\n            зчеплення. Наші експерти здійснюють заміну пошкоджених деталей,\r\n            налаштовують та тестують зчеплення для його оптимальної роботи.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійного ремонту зчеплення вашого\r\n            автомобіля. Ми забезпечимо надійну роботу зчеплення та відновимо\r\n            його ефективність, що дозволить вам насолоджуватися плавною та\r\n            безпроблемною зміною передач при керуванні автомобілем.\r\n          </p>\r\n          <ServiceComponent service={clutch} />\r\n\r\n          <h2 className={styles.service__title}>Вихлопна система</h2>\r\n\r\n          <p className={styles.service__text}>\r\n            Наша компанія спеціалізується на ремонті вихлопної системи\r\n            автомобіля, що відповідає за відведення вихлопних газів з двигуна та\r\n            зменшення шкідливих викидів в атмосферу. Вихлопна система\r\n            складається з вихлопного колектора, глушника, каталізатора (якщо\r\n            присутній), трубопроводів та інших компонентів.\r\n            <br />\r\n            <br />\r\n            Наші кваліфіковані механіки проводять діагностику вихлопної системи\r\n            для виявлення можливих проблем, таких як пошкодження глушника,\r\n            засмічення каталізатора, протікання трубопроводів або інші\r\n            несправності. Після діагностики, ми рекомендуємо необхідні ремонтні\r\n            роботи та заміну деталей для відновлення оптимальної роботи\r\n            вихлопної системи.\r\n            <br />\r\n            <br />\r\n            Ми використовуємо якісні запасні частини та сучасне обладнання для\r\n            виконання ремонту вихлопної системи. Наші експерти здійснюють заміну\r\n            пошкоджених компонентів, ремонт трубопроводів, очищення або заміну\r\n            каталізатора та інші необхідні налаштування.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійного ремонту вихлопної системи вашого\r\n            автомобіля. Ми забезпечимо надійну та ефективну роботу вихлопної\r\n            системи, зменшимо викиди шкідливих речовин та забезпечимо вам\r\n            комфортну та екологічно чисту їзду.\r\n          </p>\r\n          <ServiceComponent service={exhaust_system} />\r\n\r\n          <h2 className={styles.service__title}>Система охолодження</h2>\r\n\r\n          <p className={styles.service__text}>\r\n            Наша компанія займається ремонтом системи охолодження автомобіля,\r\n            яка відповідає за підтримання оптимальної температури двигуна та\r\n            запобігання його перегріву. Система охолодження складається з\r\n            радіатора, вентилятора, водяного насоса, термостата, шлангів та\r\n            резервуара з охолоджуючою рідиною.\r\n            <br />\r\n            <br />\r\n            Наші кваліфіковані механіки проводять діагностику системи\r\n            охолодження для виявлення можливих проблем, таких як витік\r\n            охолоджуючої рідини, несправність радіатора, неефективна робота\r\n            вентилятора, проблеми з водяним насосом або інші несправності. Після\r\n            діагностики, ми рекомендуємо необхідні ремонтні роботи та заміну\r\n            деталей для забезпечення надійної роботи системи охолодження.\r\n            <br />\r\n            <br />\r\n            Ми використовуємо якісні запасні частини, що відповідають стандартам\r\n            виробників, та сучасне обладнання для виконання ремонту системи\r\n            охолодження. Наші експерти здійснюють заміну пошкоджених\r\n            компонентів, ремонт радіатора, налагодження вентилятора та інші\r\n            необхідні регулювання.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійного ремонту системи охолодження\r\n            вашого автомобіля. Ми забезпечимо ефективне охолодження двигуна,\r\n            запобіжимо перегріву та збережемо його оптимальну роботу. Ваш\r\n            автомобіль буде готовим до подолання будь-яких дорожніх випробувань\r\n            з надійною системою охолодження.\r\n          </p>\r\n          <ServiceComponent service={cooling_system} />\r\n\r\n         \r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default OtherServices;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\TehService.jsx",["211","212"],[],"import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\nimport {\r\n  replacement_technical,\r\n  replacement_filters,\r\n  replacement_belts,\r\n  repair_ignition,\r\n} from \"./helper\";\r\nimport ServiceComponent from \"./ServiceComponent\";\r\n\r\nconst TehService = () => {\r\n  return (\r\n    <section className={styles.service__block}>\r\n      <div className={styles.service__content}>\r\n        <div className={styles.tehservice__banner}>\r\n          <h1 className={styles.service__banner_title}>\r\n            Послуги з технічного обслуговування\r\n          </h1>\r\n        </div>\r\n        <div className={styles.service__info}>\r\n          \r\n          <h2 className={styles.service__title}>\r\n            Заміна ременів та ланцюга приводу ГРМ\r\n          </h2>\r\n          <p className={styles.service__text}>\r\n            Наша компанія спеціалізується на заміні ременів та ланцюга приводу\r\n            ГРМ, що є важливою складовою системи двигуна вашого автомобіля.\r\n            Ремені та ланцюг ГРМ відповідають за синхронізацію роботи клапанів і\r\n            поршнів, забезпечуючи правильне відкриття та закриття клапанів у\r\n            встановлені часи. З часом ремені та ланцюг можуть зноситися,\r\n            розтягуватися або пошкоджуватися, що може призвести до серйозних\r\n            поломок двигуна.\r\n            <br />\r\n            <br />\r\n            Наші кваліфіковані механіки здійснюють професійну заміну ременів та\r\n            ланцюга ГРМ з використанням високоякісних запасних частин, що\r\n            відповідають стандартам виробника. Ми дотримуємося рекомендацій\r\n            виробника щодо періодичності заміни ременів та ланцюга ГРМ.\r\n            Правильна заміна гарантує правильну роботу двигуна, знижує ризик\r\n            поломок і забезпечує безпеку під час їзди.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійної заміни ременів та ланцюга приводу\r\n            ГРМ. Наші експерти забезпечать надійну заміну та оптимальну роботу\r\n            вашого двигуна, що сприятиме тривалому та безпечному функціонуванню\r\n            вашого автомобіля.\r\n          </p>\r\n          <ServiceComponent service={replacement_belts}/>\r\n\r\n          <h2 className={styles.service__title}>Ремонт системи запалювання</h2>\r\n          <p className={styles.service__text}>\r\n            Наша компанія пропонує послугу ремонту системи запалювання вашого\r\n            автомобіля, яка відіграє важливу роль у запуску та роботі двигуна.\r\n            Система запалювання включає в себе свічки запалювання, катушки\r\n            запалювання, проводи, датчики та інші компоненти, які забезпечують\r\n            створення і передачу іскри для спалаху палива.\r\n            <br />\r\n            <br />\r\n            Наші кваліфіковані техніки використовують сучасне діагностичне\r\n            обладнання для виявлення проблем у системі запалювання. Ми\r\n            займаємося виявленням та усуненням несправностей, таких як проблеми\r\n            зі свічками запалювання, пошкодження катушок, проводів або датчиків,\r\n            неправильна робота запалювання та інші проблеми, що можуть впливати\r\n            на ефективність роботи двигуна.\r\n            <br />\r\n            <br />\r\n            Після проведення діагностики і виявлення несправностей, ми\r\n            здійснюємо професійний ремонт системи запалювання, замінюючи\r\n            пошкоджені компоненти або виконуючи налагодження і налаштування\r\n            системи. Ми використовуємо лише якісні запасні частини, що\r\n            відповідають стандартам виробників.\r\n            <br />\r\n            <br />\r\n            Звертайтеся до нас для професійного ремонту системи запалювання\r\n            вашого автомобіля. Ми забезпечимо оптимальну роботу двигуна,\r\n            покращимо запуск автомобіля та забезпечимо його ефективну роботу.\r\n          </p>\r\n          <ServiceComponent service={repair_ignition}/>\r\n\r\n          <p className={styles.service__text}></p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default TehService;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\Contacts.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\Rozval.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\AutoConditioner.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\TireFitting.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Footer\\Footer.jsx",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\helper.js",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\ServiceComponent.js",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Pagination\\Pagination.js",[],[],"C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\Comments\\ReplyForm.jsx",["213","214"],[],"import React, { useState, useEffect } from \"react\";\r\nimport styles from \"../styles/Comment.module.css\";\r\n\r\nconst ReplyForm = ({ commentId, setComments }) => {\r\n  const [author, setAuthor] = useState(\"\");\r\n  const [content, setContent] = useState(\"\");\r\n  const [day, setDay] = useState(\"\");\r\n  const [email, setEmail] = useState(\"\");\r\n  const [isValid, setIsValid] = useState(false);\r\n\r\n  useEffect(() => {\r\n    const date = new Date();\r\n    const day = date.getDate();\r\n    const month = date.getMonth() + 1;\r\n    const year = date.getFullYear();\r\n    const formattedDate = `${day}.${month}.${year}`;\r\n\r\n    setDay(formattedDate);\r\n  }, []);\r\n\r\n  const handleEmailChange = (event) => {\r\n    setEmail(event.target.value);\r\n  };\r\n\r\n  useEffect(() => {\r\n    const verifyEmail = () => {\r\n      // Regular expression pattern for email validation\r\n      const pattern = /^[\\w\\.-]+@[\\w\\.-]+\\.\\w+$/;\r\n  \r\n      // Use the pattern to match the email address\r\n      if (pattern.test(email)) {\r\n        setIsValid(true);\r\n      } else {\r\n        setIsValid(false);\r\n      }\r\n    };\r\n  \r\n    verifyEmail();\r\n  }, [email])\r\n\r\n  const handleSubmit = (event) => {\r\n    event.preventDefault();\r\n\r\n    fetch(`https://sto-web-pv.onrender.com/comments/${commentId}/replies`, {\r\n      method: \"POST\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n      body: JSON.stringify({ author, content, day, email }),\r\n    })\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        // Обновление списка комментариев\r\n        setComments(data.comment.replies);\r\n        // Сброс полей формы\r\n        setAuthor(\"\");\r\n        setContent(\"\");\r\n        setEmail(\"\");\r\n      })\r\n      .catch((error) => console.error(error));\r\n  };\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit} className={styles.reply__form}>\r\n      <div className={styles.form__inputs}>\r\n        <label htmlFor=\"author\">Ім'я:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"author\"\r\n          value={author}\r\n          onChange={(event) => setAuthor(event.target.value)}\r\n          required\r\n        />\r\n      </div>\r\n      <div className={styles.form__inputs}>\r\n        <label htmlFor=\"email\">Email:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"email\"\r\n          value={email}\r\n          onChange={(event) => {\r\n            setEmail(event.target.value);\r\n            handleEmailChange(event);\r\n          }}\r\n          required\r\n        />\r\n        {!isValid ? (\r\n          <p className={email.length > 1 ? styles.red_error : styles.unActive}>\r\n            Email введений невірно*\r\n          </p>\r\n        ) : null}\r\n      </div>\r\n      <div className={styles.form__inputs}>\r\n        <label htmlFor=\"content\">Коментар:</label>\r\n        <textarea\r\n          id=\"content\"\r\n          value={content}\r\n          onChange={(event) => setContent(event.target.value)}\r\n          required\r\n        ></textarea>\r\n      </div>\r\n      <button className={styles.form__button} type=\"submit\" disabled={!isValid}>\r\n        Відповісти\r\n      </button>{\" \"}\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default ReplyForm;\r\n","C:\\Users\\user\\Desktop\\portfolio\\sto-webs\\clients\\src\\components\\SimpleLinks\\TireComponent.jsx",["215"],[],"import React from \"react\";\r\nimport styles from \"../styles/Main.module.css\";\r\n\r\nconst TireComponent